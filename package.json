{
  "name": "cbor-edn",
  "version": "0.0.0",
  "decription": "",
  "main": "index.js",
  "type": "module",
  "keywords": [
    "cbor",
    "edn",
    "Extended Diagnostic Notation",
    "parser",
    "peggy",
    "abnf"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hildjj/cbor-edn.git"
  },
  "homepage": "https://github.com/hildjj/cbor-edn#readme",
  "bugs": {
    "url": "https://github.com/hildjj/cbor-edn/issues"
  },
  "scripts": {
    "clean": "rimraf coverage docs lib",
    "lint": "eslint .",
    "parser": "peggy --dts --format es src/edn.peggy -o lib/edn.js --allowed-start-rules seq,one_item",
    "pretest": "npm run parser && tsc --inlineSourceMap",
    "test": "c8 node --test",
    "posttest": "npm run ts",
    "docs": "typedoc",
    "ts": "tsc",
    "build": "npm run test && npm run lint && npm run docs && npm pack --dry-run"
  },
  "devDependencies": {
    "@peggyjs/coverage": "1.2.0",
    "@types/node": "22.3.0",
    "abnf": "3.0.1",
    "c8": "10.1.2",
    "eslint-plugin-jsdoc": "50.2.2",
    "eslint-plugin-markdown": "5.1.0",
    "peggy": "github:peggyjs/peggy",
    "rimraf": "6.0.1",
    "typedoc": "0.26.5",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@cto.af/eslint-config": "4.1.5",
    "cbor2": "1.4.0",
    "typescript-eslint": "8.1.0"
  },
  "packageManager": "pnpm@9.7.1",
  "engines": {
    "node": ">=18"
  }
}
